{
	"name": "@move-elevator/eslint-config-typescript",
	"version": "0.1.0",
	"description": "Configuration for Eslint using TypeScript.",
	"license": "MIT",
	"author": "move elevator GmbH",
	"repository": {
		"type": "git",
		"url": "https://github.com/move-elevator/eslint-config-typescript.git"
	},
	"main": "./index.mjs",
	"files": [
		"*.json",
		"*.md",
		"*.mjs"
	],
	"scripts": {
		"fix:package:normalize": "prettier-package-json --write package.json --use-tabs"
	},
	"peerDependencies": {
		"eslint": "^9.39.2",
		"eslint-plugin-perfectionist": "^5.5.0",
		"eslint-plugin-prefer-arrow-functions": "^3.9.1",
		"typescript-eslint": "^8.55.0"
	},
	"devDependencies": {
		"prettier-package-json": "^2.8.0",
		"semantic-release": "^25.0.3"
	},
	"keywords": [
		"eslint",
		"move elevator",
		"typescript"
	],
	"publishConfig": {
		"access": "public"
	}
}
